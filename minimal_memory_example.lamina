# Minimal memory operations example
fn @main() -> void {
  entry:
    # Allocate buffer
    %buffer = alloc.stack i8

    # Store value (ASCII 'Z' = 90)
    store.i8 %buffer, 90

    # Load value back
    %loaded = load.i8 %buffer

    # Write the raw binary value to stdout using writeptr
    %result = writeptr %buffer

    # Print newline
    %nl = writebyte 10

    ret.void
}







