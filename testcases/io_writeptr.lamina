# Test writeptr I/O operation setup
fn @main() -> i32 {
  entry:
    # Allocate a buffer and initialize it
    %buffer = alloc.stack i8
    store.i8 %buffer, 79  # Store 'O' (ASCII 79)

    # Write the buffer value to stdout using writeptr (produces 'O')
    %result = writeptr %buffer

    # Write the rest using writebyte (produces 'K\n')
    %k = writebyte 75 # 'K'
    %newline = writebyte 10  # '\n'

    ret.i32 0
}
