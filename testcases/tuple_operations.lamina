# Simple tuple test
fn @test_basic_tuple() -> i64 {
  entry:
    %tuple = tuple 42, 1
    %number = extract.tuple %tuple, 0
    %other = extract.tuple %tuple, 1
    %result = add.i64 %number, %other
    ret.i64 %result
}

fn @main() -> i64 {
  entry:
    %basic_result = call @test_basic_tuple()
    print %basic_result
    ret.i64 0
}